{
	"mainEntryPointFilePath": "",
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/tsconfig.es6.json"
	},
	"apiReport": {
		"enabled": true,
		"reportFolder": "<projectFolder>",
		"includeForgottenExports": true
	},
	"docModel": {
		"enabled": true,
		"projectFolderUrl": "http://github.com/Nernar/nernar.github.io/tree/master/api"
	},
	"tsdocMetadata": {
		"enabled": true
	},
	"dtsRollup": {
		"enabled": false
	},
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "warning",
				"addToApiReportFile": true
			}
		},
		"extractorMessageReporting": {
			"default": {
				"logLevel": "warning",
				"addToApiReportFile": true
			},
			"ae-missing-release-tag": {
				"logLevel": "none"
			}
		},
		"tsdocMessageReporting": {
			"default": {
				"logLevel": "warning",
				"addToApiReportFile": true
			},
			"tsdoc-link-tag-unescaped-text": {
				"logLevel": "warning",
				"addToApiReportFile": true
			},
			"tsdoc-param-tag-missing-hyphen": {
				"logLevel": "none"
			}
		}
	}
}
