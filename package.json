{
  "name": "innercore-api",
  "version": "2.3.1b115",
  "author": "Nernar",
  "description": "Inner Core API Reference",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nernar/nernar.github.io.git#master"
  },
  "bugs": {
    "url": "https://github.com/Nernar/nernar.github.io/issues"
  },
  "homepage": "https://t.me/ntInsideChat",
  "license": "GPL-3.0-or-later",
  "workspaces": [
    "./typedoc-codicon-theme"
  ],
  "private": true,
  "scripts": {
    "prebuild-declarations": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "prebuild-theme": {
      "command": "npm run --prefix typedoc-codicon-theme build",
      "files": [
        "typedoc-codicon-theme/**/*"
      ],
      "output": [
        "typedoc-codicon-theme/dist"
      ]
    },
    "prebuild-declarations": {
      "command": "tsx tools/rebuild_types.ts core-engine",
      "files": [
        "tsconfig.json",
        "tools/rebuild_types.ts",
        "core-engine/**/*.d.ts"
      ],
      "output": [
        "declarations/core-engine.d.ts"
      ]
    },
    "build": {
      "command": "typedoc declarations/core-engine.d.ts",
      "files": [
        "typedoc.json",
        "tsconfig-typedoc.json",
        "declarations/core-engine.d.ts",
        "typedoc-codicon-theme/**/*"
      ],
      "dependencies": [
        "prebuild-theme",
        "prebuild-declarations"
      ],
      "output": [
        "docs"
      ]
    }
  },
  "devDependencies": {
    "glob": "^10.2.6",
    "tsx": "^3.12.7",
    "wireit": "^0.9.5"
  },
  "dependencies": {
    "typedoc": "^0.24.8"
  }
}
