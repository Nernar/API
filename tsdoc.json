{
	"$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
	"reportUnsupportedHtmlElements": true,
	"noStandardTags": true,
	"tagDefinitions": [
		{
			"tagName": "@clientside",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@defaultValue",
			"syntaxKind": "block"
		},
		{
			"tagName": "@decorator",
			"syntaxKind": "block",
			"allowMultiple": true
		},
		{
			"tagName": "@deprecated",
			"syntaxKind": "block"
		},
		{
			"tagName": "@eventProperty",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@example",
			"syntaxKind": "block",
			"allowMultiple": true
		},
		{
			"tagName": "@experimental",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@inheritDoc",
			"syntaxKind": "inline"
		},
		{
			"tagName": "@internal",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@internalRemarks",
			"syntaxKind": "block"
		},
		{
			"tagName": "@label",
			"syntaxKind": "inline"
		},
		{
			"tagName": "@link",
			"syntaxKind": "inline",
			"allowMultiple": true
		},
		{
			"tagName": "@override",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@packageDocumentation",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@page",
			"syntaxKind": "inline",
			"allowMultiple": true
		},
		{
			"tagName": "@param",
			"syntaxKind": "block",
			"allowMultiple": true
		},
		{
			"tagName": "@preapproved",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@privateRemarks",
			"syntaxKind": "block"
		},
		{
			"tagName": "@readonly",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@remarks",
			"syntaxKind": "block"
		},
		{
			"tagName": "@returns",
			"syntaxKind": "block"
		},
		{
			"tagName": "@sealed",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@see",
			"syntaxKind": "block"
		},
		{
			"tagName": "@since",
			"syntaxKind": "block"
		},
		{
			"tagName": "@throws",
			"syntaxKind": "block",
			"allowMultiple": true
		},
		{
			"tagName": "@todo",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@typeParam",
			"syntaxKind": "block",
			"allowMultiple": true
		},
		{
			"tagName": "@virtual",
			"syntaxKind": "modifier"
		}
	],
	"supportForTags": {
		"@clientside": true,
		"@defaultValue": true,
		"@decorator": true,
		"@deprecated": true,
		"@eventProperty": true,
		"@example": true,
		"@experimental": true,
		"@inheritDoc": true,
		"@internal": true,
		"@internalRemarks": true,
		"@label": true,
		"@link": true,
		"@override": true,
		"@packageDocumentation": true,
		"@page": true,
		"@param": true,
		"@preapproved": true,
		"@privateRemarks": true,
		"@readonly": true,
		"@remarks": true,
		"@returns": true,
		"@sealed": true,
		"@see": true,
		"@since": true,
		"@throws": true,
		"@todo": true,
		"@typeParam": true,
		"@virtual": true
	}
}
